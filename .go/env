export GO111MODULE="on"
export GOPATH="$HOME/.go/GOPATH"
export GOPROXY="https://goproxy.cn,https://goproxy.io,direct"
export PATH="$HOME/.go/bin:$HOME/.go/current/bin:$HOME/.go/GOPATH/bin:$PATH"

alias gobuild='CGO_ENABLED=0 GOOS=linux GOPROXY=https://goproxy.cn,direct go build -ldflags="-s -w" -o '
